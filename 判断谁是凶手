#define  _CRT_SECURE_NO_WARNINGS
#include <stdio.h>
//A说不是我；
//B说是C；
//C说是D；
//D说C在胡说；
//已知三人说了真话，1人说了假话；
//谁是凶手？
int main()
{
	char killer = 0;
	for (killer = 'a';killer <= 'd';killer++)
	{
		if ((killer != 'a') + (killer == 'c') + (killer == 'd') + (killer != 'd') == 3)
		{
			printf("%c\n", killer);
		}
	}
	return 0;
}
